<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressBar">
        <attr name="progress_color" format="color|reference" />
        <attr name="progress_secondary_color" format="color|reference" />
        <attr name="progress_base_color" format="color|reference" />
        <attr name="progress_stroke_width" format="dimension|reference" />
        <attr name="progress_duration" format="integer|reference" />
        <attr name="progress_round_style" format="boolean|reference" />
        <attr name="progress_is_increment" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="SimpleCircleProgressBar">
        <attr name="simple_progress_color" format="color|reference" />
        <attr name="simple_progress_background_color" format="color|reference" />
        <attr name="simple_progress_stroke_width" format="dimension|reference" />
    </declare-styleable>
</resources>
